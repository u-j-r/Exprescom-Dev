<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_l10n_mx_edi_fuel_complement_product_form_inherit" model="ir.ui.view">
        <field name="name">l10n_mx_edi.fuel.complement.product.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="account.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='invoicing']" position="inside">
                <group>
                    <group string="Fuel Consumption">
                        <field name="l10n_mx_edi_fuel_billing"/>
                    </group>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
